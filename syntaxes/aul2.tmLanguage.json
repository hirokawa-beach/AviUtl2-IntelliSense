{
  "scopeName": "source.lua",
  "patterns": [
    {
      "name": "keyword.aul2.track",
      "match": "--(track|check|color|file|font|figure|select|value|label|script|information)@\\w*:"
    },
    {
      "name": "comment.line.double-dash.lua",
      "match": "--.*$"
    },
    {
      "name": "support.function.obj.lua",
      "match": "\\bobj\\.(draw|load|effect|getvalue|getoption|setoption|setanchor|getaudio|getpixel|pixelshader|rand|mes|clearbuffer|interpolation)\\b"
    }
  ],
  "fileTypes": ["anm2", "obj2", "cam2", "scn2", "tra2"],
  "name": "AviUtl2 Lua"
}